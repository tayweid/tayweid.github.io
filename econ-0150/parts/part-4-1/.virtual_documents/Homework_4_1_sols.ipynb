





# Imports
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
import statsmodels.formula.api as smf

# File Path
file_path = 'https://tayweid.github.io/econ-0150/parts/part-4-1/data/'


























# Load Data
data = pd.read_csv(file_path + 'BRFSS_cleaned.csv')
data.head()


# regression: BMI5 ~ unemployment_rate

results = smf.ols(formula = 'BMI5 ~ unemployment_rate', data = data).fit()
print(results.summary().tables[1])











# regression: MENTHLTH ~ unemployment_rate

results = smf.ols(formula = 'MENTHLTH ~ unemployment_rate', data = data).fit()
print(results.summary().tables[1])
